datasource db {
    provider = "sqlite"
    url = "***"
}

generator client {
    provider = "prisma-client-js"
}

model Todo {
    id          Int     @id @default(autoincrement())
    name        String
    isCompleted Boolean
    order       Int?
}

model User {
    id        Int      @id @default(autoincrement())
    createdAt DateTime @default(now())
    firstName String
    email     String
}
